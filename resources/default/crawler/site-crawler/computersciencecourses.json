{
  "$id": "computersciencecourses",
  "$schema": "https://schema.yext.com/config/crawler/site-crawler/v1",
  "name": "Computer Science Courses",
  "enabled": true,
  "crawlSchedule": "once",
  "crawlStrategy": "subPages",
  "domains": [
    "https://www.cs.princeton.edu"
